{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "frameworks/**/*",
          "adapters/**/*",
          "config/**/*",
          "usecases/**/*",
          "utils/**/*",
          "middlewares/**/*"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ],
  "regions": ["gru1"]
}
